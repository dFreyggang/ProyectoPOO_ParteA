public class Evaluacion{
    
	//ATRIBUTOS
	private String asignatura;
	private int nota;

	//-------------------------------------------------------------------------------------------------------------------------------------//

	//CONSTRUCTOR
	public Evaluacion(String asignatura, int nota){
		this.asignatura = asignatura;
		this.nota = nota;
	}

	//-------------------------------------------------------------------------------------------------------------------------------------//

	//GETTERS
	public String getAsignatura(){
		return asignatura;
	}
	public int getNota(){
		return nota;
	}
	//-------------------------------------------------------------------------------------------------------------------------------------//

	//SETTERS
	public void setAsignatura(String codigo){
		this.asignatura=asignatura;
	}
	public void setNota(int nota){
		this.nota=nota;
	}
	//-------------------------------------------------------------------------------------------------------------------------------------//

	//METODOS
	
}

	

