public class Alumno {

    private String rut;
    private String nombre;
    private String email;
    private int telefono;
    private ListaAsignaturaPorAlumno listaAsignatura[]; //NO COMPACTA
    private ListaEvaluaciones listaEvaluaciones[]; //NO COMPACTA

    public Alumno (String rut, String nombre, String email, int telefono, int cantAsignaturas, int cantEvaluacines) {
        this.rut = rut;
        this.nombre = nombre;
        this.email = email;
        this.telefono = telefono;
        this.listaAsignatura = new ListaAsignaturaPorAlumno[cantAsignaturas];
        this.listaEvaluaciones = new ListaEvaluaciones[cantEvaluacines];
    }
}